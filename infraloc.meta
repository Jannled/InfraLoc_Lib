{
	"names": {
		"microxrcedds_client": {
			"cmake-args": [
				"-DUCLIENT_SERIAL_TRANSPORT_MTU=512",
				"-DUCLIENT_CUSTOM_TRANSPORT_MTU=512",
				"-DUCLIENT_HARD_LIVELINESS_CHECK=ON",
                "-DUCLIENT_HARD_LIVELINESS_CHECK_TIMEOUT=5000"
			]
		},
		"rmw_microxrcedds": {
			"cmake-args": [
				"-DRMW_UXRCE_MAX_PUBLISHERS=10",
				"-DRMW_UXRCE_MAX_SERVICES=8"
			]
		},
		"rcutils": {
			"cmake-args": [
				"-DRCUTILS_AVOID_DYNAMIC_ALLOCATION=OFF"
			]
		}
	}
}